{
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET"],
      "allowedRoles": ["registeredusers"]
    },
    {
      "route": "/api/*",
      "methods": ["PUT", "POST", "PATCH", "DELETE"],
      "allowedRoles": ["administrator"]
    },
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/departments",
      "redirect": "/api/departments/getall"
    },
    {
      "route": "/products",
      "redirect": "/api/products/getall"
    },
    {
      "route": "/subpieces",
      "redirect": "/api/subpieces/getall"
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "responseOverrides": {
    "400": {
      "rewrite": "/invalid-invitation-error.html"
    }


  },
  "mimeTypes": {
    ".json": "text/json"
  }
}