{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous", "authenticated"],
      "rewrite": "/api/{*}"
    },
    {
      "route": "/departments",
      "allowedRoles": ["anonymous", "authenticated"],
      "rewrite": "/api/departments/getall"
    },
    {
      "route": "/*",
      "allowedRoles": ["anonymous", "authenticated"],
      "serve": "/index.html"
    }
  ]
}